{
  "name": "three-bmfont-text",
  "version": "2.2.1",
  "description": "renders BMFont files in ThreeJS with word-wrapping",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Matt DesLauriers",
    "email": "dave.des@gmail.com",
    "url": "https://github.com/mattdesl"
  },
  "standard": {
    "globals": [
      "THREE"
    ]
  },
  "dependencies": {
    "array-shuffle": "^1.0.1",
    "as-number": "^1.0.0",
    "inherits": "^2.0.1",
    "layout-bmfont-text": "^1.2.0",
    "nice-color-palettes": "^2.0.0",
    "object-assign": "^4.0.1",
    "quad-indices": "^2.0.1",
    "three-buffer-vertex-data": "^1.0.0",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "bluebird": "^3.5.1",
    "browserify": "^14.5.0",
    "budo": "^10.0.4",
    "flatten-vertex-data": "^1.0.0",
    "glsl-aastep": "^1.0.1",
    "glsl-noise": "0.0.0",
    "glslify": "^6.1.0",
    "load-bmfont": "^1.0.0",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-serve": "^0.4.1",
    "rollup-plugin-uglify": "^2.0.1",
    "standard": "^10.0.3",
    "sun-tzu-quotes": "^1.0.0",
    "three": "^0.88.0",
    "three-orbit-controls": "^82.1.0",
    "three-orbit-viewer": "^69.2.9",
    "three-vignette-background": "^1.0.2",
    "uglify-js": "^3.2.0",
    "word-wrapper": "^1.0.7"
  },
  "scripts": {
    "build": "rollup -c",
    "build2": "browserify test/test-shader.js -t glslify | uglifyjs -cm > test/bundle.js",
    "build-debug": "browserify main.js > test/bundle-debug.js",
    "build-lib": "browserify main.js | uglifyjs -cm > test/three-bmfont-text.min.js",
    "start": "budo test/test-shader.js:bundle.js --dir test --live -- -t glslify",
    "start-test": "budo test/test.js:bundle.js --dir test --live -- -t glslify",
    "test-2d": "budo test/test-2d.js:bundle.js --dir test --live",
    "test-3d": "budo test/test-3d.js:bundle.js --dir test --live",
    "test-multi": "budo test/test-multi.js:bundle.js --dir test --live",
    "test-msdf2": "budo test/test-msdf.js:bundle.js --dir test --live",
    "test-msdf": " rollup --config rollup.serve.config.js -i test/test-msdf.js",
    "test": "standard"
  },
  "keywords": [
    "bmfont",
    "bitmap",
    "font",
    "text",
    "three",
    "threejs",
    "quad",
    "quads",
    "batch",
    "render",
    "fonts",
    "layout",
    "word",
    "wrap",
    "word-wrap",
    "word-wrapping",
    "wrapping",
    "glyph",
    "glyphs"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Jam3/three-bmfont-text.git"
  },
  "homepage": "https://github.com/Jam3/three-bmfont-text",
  "bugs": {
    "url": "https://github.com/Jam3/three-bmfont-text/issues"
  }
}
